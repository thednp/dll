{
  "name": "@thednp/dll",
  "version": "1.0.0",
  "author": "thednp",
  "license": "MIT",
  "description": "Double Lazy Load for Images, Videos and Background Images.",
  "homepage": "https://thednp.github.io/dll",
  "source": "src/index.ts",
  "main": "dist/dll.js",
  "module": "dist/dll.cjs",
  "esnext": "dist/dll.mjs",
  "types": "dist/dll.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\"",
    "fix:ts": "eslint src --config .eslintrc.cjs --fix",
    "lint:ts": "eslint src --config .eslintrc.cjs",
    "build": "npm run lint:ts && npm run build-vite && npm run dts",
    "build-vite": "vite build && npm run docs",
    "dts": "dts-bundle-generator --config ./dts.config.ts",
    "docs": "ncp dist/dll.js docs/assets/js/dll.js && ncp dist/dll.js.map docs/assets/js/dll.js.map"
  },
  "bugs": {
    "url": "https://github.com/thednp/dll/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/dll.git"
  },
  "keywords": [
    "dll.js",
    "lazy-load",
    "lazy-load-image",
    "lazy-load-video",
    "double-lazy-load",
    "typescript"
  ],
  "dependencies": {
    "@thednp/shorty": "^2.0.0-alpha12"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.30.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}
