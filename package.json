{
  "name": "dll.js",
  "version": "1.5.7-alpha3",
  "description": "Double Lazy Load for Images, Videos and Background Images.",
  "main": "dist/dll.min.js",
  "module": "dist/dll.esm.js",
  "jsnext": "src/dll.js",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix:js": "eslint src/ --config .eslintrc --fix",
    "lint:js": "eslint src/ --config .eslintrc",
    "build": "npm run lint:js && npm-run-all --parallel build-*",
    "build:ts": "tsc -d",
    "build-esm": "rollup --environment FORMAT:esm,MIN:false -c",
    "build-esm-min": "rollup --environment FORMAT:esm,MIN:true -c",
    "build-umd": "rollup --environment FORMAT:umd,MIN:false -c",
    "build-umd-min": "rollup --environment FORMAT:umd,MIN:true -c",
    "build-es5-umd": "rollup --environment FORMAT:umd,MIN:false,ES:es5 -c",
    "build-es5-umd-min": "rollup --environment FORMAT:umd,MIN:true,ES:es5 -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/dll.js.git"
  },
  "keywords": [
    "dll.js",
    "lazy load",
    "lazy load image",
    "lazy load video",
    "double lazy load",
    "vanilla js",
    "vanilla javascript"
  ],
  "author": "thednp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thednp/dll.js/issues"
  },
  "homepage": "https://thednp.github.io/dll.js/",
  "dependencies": {
    "shorter-js": "^0.3.0alpha21"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.7.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.32.1",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^4.5.2"
  }
}
