{
  "name": "@thednp/dll",
  "version": "1.0.6",
  "description": "Double Lazy Load for Images, Videos and Background Images.",
  "homepage": "https://thednp.github.io/dll",
  "author": "thednp",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./dist/dll.js",
  "module": "./dist/dll.mjs",
  "types": "./dist/dll.d.ts",
  "exports": {
    ".": {
      "require": "./dist/dll.cjs",
      "import": "./dist/dll.mjs"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\"",
    "fix:ts": "eslint src --config .eslintrc.cjs --fix",
    "lint:ts": "eslint src --config .eslintrc.cjs",
    "build": "npm run lint:ts && npm run build-vite && npm run build-dts",
    "build-vite": "vite build && npm run build-docs",
    "build-dts": "dts-bundle-generator --config ./dts.config.ts",
    "build-docs": "ncp dist/dll.js docs/assets/js/dll.js && ncp dist/dll.js.map docs/assets/js/dll.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/dll.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "dll",
    "lazy-load",
    "image",
    "video",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/thednp/dll/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.36.0",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "ncp": "^2.0.0",
    "prettier": "^2.8.4",
    "typescript": "^5.0.2",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@thednp/shorty": "^2.0.0-alpha14"
  }
}
